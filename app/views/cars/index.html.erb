<h1>Hello</h1>

<%= @cars.each do |car| %>
<div class="card" style="width: 18rem;">
  <img src="..." class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"> <% car.brand %></h5>
    <p class="card-text"><%= car.model %><%= car.price %></p>
    <%= link_to "Check car", car_path(:id => car.id) %>
  </div>
</div>
<%end%>
